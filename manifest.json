  "manifest_version": 3,
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "popup/hello.html"
  }
      "matches": ["<all_urls>"],
    "js": ["scripts/scripts.ts, popup/popup.ts"],
    "mime": "text/typescript"
}
